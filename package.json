{
  "name": "react-vite-boilerplate",
  "version": "1.0.0",
  "description": "ReactJS & Vite boilerplate",
  "main": "main.tsx",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "dev": "vite --host",
    "test": "vitest run",
    "test:interactive": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "vite preview",
    "tsc": "tsc",
    "build": "tsc && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grimowsky/react-vite-boilerplate.git"
  },
  "keywords": [
    "react",
    "vite",
    "boilerplate"
  ],
  "author": "Grimm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grimowsky/react-vite-boilerplate/issues"
  },
  "homepage": "https://github.com/Grimowsky/react-vite-boilerplate#readme",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-istanbul": "^0.32.4",
    "@vitest/ui": "^0.31.4",
    "autoprefixer": "^10.4.20",
    "eslint-config-grimowsky": "^1.0.8",
    "history": "^5.3.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "5.0.3",
    "vite": "^4.5.5",
    "vite-plugin-svgr": "^3.3.0",
    "vitest": "^0.32.4"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-grimowsky"
    ],
    "root": true
  }
}
